{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["dom", "dom.iterable", "es2022"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": true
	}
}

